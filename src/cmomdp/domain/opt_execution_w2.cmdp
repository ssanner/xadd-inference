            ([price + (0.165 * 1.0) + 0.27])
            ([price + (0.165 * 1.0) - 0.135])
        )
    inventory' (    [0])
    w2' ([w2])
reward
    (   [inventory > 0.001]
        (   [1 * inventory] )
        (   [0] )
    )
+
        ([ w2 * ((1.0 * inventory) * price - (inventory * s0))])
endaction

discount 0.95
iterations 5